# PhoneInfo Configuration
# Copy this file to .env and fill in your values

# ===================
# Required Settings
# ===================

# ME API Configuration
ME_API_URL=https://app.mobile.me.app/business-api/search
ME_API_SID=your_sid_here
ME_API_TOKEN=your_token_here

# SYNC API Configuration (optional)
# SYNC_API_URL=https://callerid.powerlead.com/api/third/search
# SYNC_API_TOKEN=your_sync_token_here

# ===================
# Optional Settings
# ===================

# Database file path (default: db/db.db)
# DATABASE=db/db.db

# Server configuration
# HOST=0.0.0.0
# PORT=5001

# Debug mode (default: true, set to false for production)
# DEBUG=true

# ===================
# Security Settings
# ===================

# Session Secret Key (for Flask sessions)
# Will be auto-generated and stored in database if not set
# SECRET_KEY=your_secret_key_here

# ===================
# Logging Settings
# ===================

# Log retention in days (default: 90)
# LOG_RETENTION_DAYS=90
